<%
	var vo = data;
%>
<tr>
	<td><%=vo.no%></td>
	<td><%=vo.name%></td>
	<td><%=vo.count%></td>
	<td><%=vo.description%></td>
	<%
		if(vo.count == 0 && vo.name != '미분류'){
	%>
		<td>
			<form  action="${pageContext.request.contextPath}/api/${authUser.id}/admin/category/delete" method="post" >
				<input type='hidden' name="no" value="<%= vo.no %>">
				<input type="image" src="/assets/images/delete.jpg">
			</form>
		</td>
	<% 
		} else if (vo.count > 0 || vo.name == '미분류') {
	%>
		<td></td>
	<%
		}
	%>
</tr>